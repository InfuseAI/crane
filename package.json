{
  "name": "crane",
  "productName": "Crane",
  "version": "2.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.16.13",
    "craco-less": "^1.20.0",
    "dockerode": "^3.3.1",
    "electron": "^15.0.0",
    "electron-is-dev": "^1.2.0",
    "filesize.js": "^2.0.0",
    "keytar": "^7.7.0",
    "node-ipc": "^9.1.3",
    "parse-markdown-table": "^0.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-lazylog": "^4.5.3",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "timeago.js": "^4.0.2",
    "uuid": "^8.3.1",
    "web-vitals": "^1.0.1"
  },
  "main": "public/index.js",
  "scripts": {
    "start:ui": "craco start",
    "start:electron": "electron .",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write .",
    "dev": "concurrently -k -n webpack,electron \"cross-env BROWSER=none PORT=16888 yarn start:ui\" \"yarn electron\"",
    "electron": "wait-on tcp:16888 && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "circular-dependency-plugin": "^5.2.2",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "prettier": "2.4.1",
    "simple-progress-webpack-plugin": "^2.0.0",
    "wait-on": "^6.0.0",
    "webpackbar": "^5.0.0-3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  }
}
